{
  "expo": {
    "name": "CrediStore",
    "slug": "credistore-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "scheme": "credistoreapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/2f5924ca-1366-47c8-84ff-7142aed4637c"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jeffersonquezadadev.credistoreapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/logo.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.jeffersonquezadadev.credistoreapp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#4338ca",
          "backgroundImage": "./assets/images/splash-background.png",
          "dark": {
            "backgroundColor": "#1e1b4b",
            "backgroundImage": "./assets/images/splash-background.png"
          }
        }
      ],
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2f5924ca-1366-47c8-84ff-7142aed4637c"
      }
    }
  }
}
